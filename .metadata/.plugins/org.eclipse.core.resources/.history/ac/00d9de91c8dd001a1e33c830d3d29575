package notas;

public class Estudante {
	
	public double nota1;
	public double nota2;
	public double nota3;
	public double nome;
	
	public double notaFinal() {
		
		return (nota1 + nota2 + nota3);
	}
	
	public boolean passou() {
		
		return (this.notaFinal()=60);
	}
	
	public double calcularDiagonal() {
		
		return Math.sqrt(Math.pow(ladoA, 2)+Math.pow(ladoB, 2));
	}

}
