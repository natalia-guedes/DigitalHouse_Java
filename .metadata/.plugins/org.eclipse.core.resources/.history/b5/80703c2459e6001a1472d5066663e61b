package banco;

import contato.Endereço;

public class Clientes {
	private Integer id;
	private String nome;
	private String rg;
	private String cpf;
	private String sobrenome;

	public Clientes(Integer id, String nome, String rg, String cpf, String sobrenome) {
		this.id=id;
		this.nome=nome;
		this.rg=rg;
		this.cpf=cpf;
		this.sobrenome=sobrenome;
	}

}
